{
  "name": "plug",
  "version": "0.0.0",
  "description": "Your plug into the Internet Computer",
  "private": true,
  "repository": "https://github.com/FleekHQ/plug-extension",
  "author": {
    "name": "Christian Laino",
    "email": "christian@fleek.co",
